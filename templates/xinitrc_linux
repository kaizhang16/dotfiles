# Virtualbox
VBoxClient-all

# Xresources
[[ -f ~/.Xresources ]] && xrdb -merge ~/.Xresources

# Trayer
trayer --edge top --align right --widthtype request --heighttype request --SetDockType true --transparent true &

# Fcitx
export GTK_IM_MODULE=fcitx
export QT_IM_MODULE=fcitx
export XMODIFIERS=@im=fcitx
fcitx &

# Albert
albert &

# xmonad
exec xmonad
